{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],

    "MinimumLevel": {
      "Default": "Information",

      "Override": {
        "Microsoft": "Information"
      }
    },

    "WriteTo": [
      { "Name": "Console" }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server = localhost; Port=2345; Database=db; User Id=postgres; Password="
  },

  "Http": { "TimeoutSeconds": 10 },

  "RabbitMq": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "Exchange": ""
  }
}
