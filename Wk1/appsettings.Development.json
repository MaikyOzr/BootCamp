{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console"],

    "MinimumLevel": {
      "Default": "Information",

      "Override": {
        "Microsoft": "Information"
      }
    },

    "WriteTo": [
      { "Name": "Console" }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server = localhost; Port=5432; Database=db; User Id=postgres; Password="
  },

  "Http": { "TimeoutSeconds": 10 },

  "Jwt": {
    "Issuer": "https://localhost:7157",
    "Audience": "https://localhost:7157",
    "SecretKey": ""
  }
}
